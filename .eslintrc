{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "indent": "off",
    "comma-dangle": "off",
    "object-curly-spacing": "off",
    "require-jsdoc": "off",
    "space-before-function-paren": "off",
    "max-len": "off"
  },
  "extends": [
    "eslint:recommended", "google"
  ]
}
